{
  "name": "twitter noise remover",
  "description": "remove retweet with flaming, trolling and so on",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_icon": "icon.png"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["jquery.min.js", "js.js"]
    }
  ]
}


